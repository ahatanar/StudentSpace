# Google Calendar Integration
# Get these from Google Cloud Console > APIs & Services > Credentials
GOOGLE_CALENDAR_CLIENT_ID=your-client-id-here.apps.googleusercontent.com
GOOGLE_CALENDAR_CLIENT_SECRET=your-client-secret-here
GOOGLE_CALENDAR_REDIRECT_URI=http://localhost:8000/api/calendar/oauth-callback

# Frontend URL for OAuth redirects
FRONTEND_APP_URL=http://localhost:3000

# Token encryption key (generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())")
# For now, leave empty - it will auto-generate from client secret
CALENDAR_ENCRYPTION_KEY=
